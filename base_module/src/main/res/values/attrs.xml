<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="Toolbar">
        <attr name="share_visible" format="boolean"/>
        <attr name="navigation_icon" format="integer"/>
    </declare-styleable>

    <declare-styleable name="ExpandableTextView">
        <attr name="expendLabel" format="string"/>
        <attr name="expendableLabelColor" format="color"/>
        <attr name="collapsingLabel" format="string"/>
        <attr name="collapsingLabelColor" format="color"/>
        <attr name="maxCollapsedLines" format="integer"/>
        <attr name="contentStyle" format="integer"/>
    </declare-styleable>
</resources>